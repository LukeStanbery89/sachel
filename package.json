{
    "name": "@lukestanbery/satchel",
    "version": "1.0.2",
    "description": "Simple and lightweight package for caching data to a local key value store.",
    "source": "src/index.js",
    "main": "dist/index.js",
    "targets": {
        "main": {
            "optimize": true
        }
    },
    "scripts": {
        "build": "parcel build",
        "postinstall": "if [ -e ./scripts/postinstall.sh ]; then ./scripts/postinstall.sh; fi",
        "prepublishOnly": "npm run build",
        "test-unit": "jest --coverage=true ./test/unit --verbose --runInBand"
    },
    "files": [
        "dist/*"
    ],
    "author": "Luke Stanbery",
    "license": "MIT",
    "devDependencies": {
        "@jest/types": "^29.0.2",
        "jest": "^29.0.2",
        "node-notifier": "^10.0.1",
        "parcel": "^2.7.0"
    }
}